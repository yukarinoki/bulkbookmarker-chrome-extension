 {
    "name": "Bulk-Bookmarker",
    "version": "2.0",
    "description": "Bookmark All tab and Can Open It",
    "permissions":[
      "bookmarks",
      "tabs",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
 }
